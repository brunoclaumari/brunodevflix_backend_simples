{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/categorias",
            "dest": "server.js",
            "methods": [
                "GET", "POST"
            ]
        },
        {
            "src": "/videos",
            "dest": "./server.js",
            "methods": [
                "GET","POST"
            ]
        }
    ],
    "functions": {
        "./server.js": {
          "memory": 1024,
          "includeFiles": "./db.json"
        }
      }      
}